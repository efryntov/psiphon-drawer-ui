    <!--  Summary text for 'Add custom HTTP headers' checkbox, appears underneath the checkbox title -->
    <string name="custom_proxy_headers_checkbox_description">Tüm giden HTTP proxy taleplerine özel HTTP üst bilgileri ekleyin</string>
    <string name="custom_proxy_header_group_1">Üst bilgi 1</string>
    <string name="custom_proxy_header_group_2">Üst bilgi 2</string>
    <string name="custom_proxy_header_group_3">Üst bilgi 3</string>
    <string name="custom_proxy_header_group_4">Üst bilgi 4</string>
