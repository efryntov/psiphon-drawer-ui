    <!--  Summary text for 'Add custom HTTP headers' checkbox, appears underneath the checkbox title -->
    <string name="custom_proxy_headers_checkbox_description">أضف عناوين HTTP خاصة لكل طلبات بروكسي HTTP الخارجة </string>
    <string name="custom_proxy_header_group_1">عنوان 1 </string>
    <string name="custom_proxy_header_group_2">عنوان 2</string>
    <string name="custom_proxy_header_group_3">عنوان 3 </string>
    <string name="custom_proxy_header_group_4">عنوان 4</string>
    <string name="custom_proxy_header_group_5">عنوان 5</string>
    <string name="custom_proxy_header_group_6">عنوان 6</string>
